;Write a program that defines symbolic constants for all seven days of the week.
;Create an array variable that uses the symbols as initalizers. 

INCLUDE \masm32\include\masm32rt.inc
INCLUDE \masm32\include\Irvine32.inc
INCLUDELIB \masm32\lib\Irvine32.lib
INCLUDE \masm32\include\debug.inc
INCLUDELIB \masm32\lib\debug.lib
.486
;.model flat, stdcall
.stack 1000h
ExitProcess PROTO, DwErrorCode:DWORD
WriteHex PROTO
Crlf PROTO
;Write a program that defines symbolic constants for all seven days of week.
;Create an array variable that uses the symbols as initializers.
;Dump the memory.
SUN = 0
MON = 1
TUE = 2
WED = 3
THU = 4
FRI = 5
SAT = 6
.data
daysArray BYTE SUN,MON,TUE,WED,THU,FRI,SAT
.code
Call WriteHex
main PROC
inkey ; then console build should be performed
INVOKE ExitProcess, 0
main ENDP
END main
